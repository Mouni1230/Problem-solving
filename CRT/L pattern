#include <stdio.h>

int main()
{
    int n;
  printf("enter value:");
  scanf("%d",&n);
  for(int i=1;i<=n;i++)
  {
      for(int j=1;j<=n;j++)
      {
          if(i==n||j==1||i==j)
          {
          printf("%d",i);
          }
          else
          {
              printf(" ");
          }
      }
 

   printf("\n");
}
    return 0;
}
