#include <stdio.h>

int main()
{
    float n,sum;
    printf("Enter number:");
    scanf("%f",&n);
    for(int i=1;i<=n;i++)
    {
        double nr=1.0;
        long long dr=1;
        for(int j=1;j<=i;j++)
        {
            nr *=i;
        }
        for(int j=1;j<=i;j++)
        {
            dr *=j;
        }
        sum+=nr/dr;
        
    
    }
    printf("%f ",sum);
    
    
    
    return 0;
}
