#include <stdio.h>

int main()
{
    int n;
    printf("Enter values in array:");
    scanf("%d",&n);
    int arr[n];
    
    for(int i=1;i<=n;i++)
    {
        scanf("%d",&arr[i]);
    }
    int big=arr[0];
    for(int j=1;j<=n;j++)
    {
        if(arr[j]>big)
        {
            big=arr[j];
        }
    }
  printf("The Biggest element is :%d",big);
    return 0;
}
